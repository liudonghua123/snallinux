[options]
audible_bell = true
clickable_url = true
dynamic_title = false
font = Monospace 16
fullscreen = false
icon_name = termite
mouse_autohide = false
scrollback_lines = 10000
hyperlinks = true

# $BROWSER is used by default if set, with xdg-open as a fallback
browser = chromium

# "system", "on" or "off"
cursor_blink = on

# "block", "underline" or "ibeam"
cursor_shape = block

# set size hints for the window
#size_hints = false

# "off", "left" or "right"
#scrollbar = off

[colors]

# special
foreground      = #93a1a1
foreground_bold = #93a1a1
cursor          = #93a1a1
background      = #002b36

# black
color0  = #002b36
color8  = #657b83

# red
color1  = #dc322f
color9  = #dc322f

# green
color2  = #859900
color10 = #859900

# yellow
color3  = #b58900
color11 = #b58900

# blue
color4  = #268bd2
color12 = #268bd2

# magenta
color5  = #6c71c4
color13 = #6c71c4

# cyan
color6  = #2aa198
color14 = #2aa198

# white
color7  = #93a1a1
color15 = #fdf6e3


[hints]
font = Monospace 16
foreground      = #93a1a1
background      = #002b36
active_foreground = #e68080
active_background = #3f3f3f
padding = 0
border = #3f3f3f
border_width = 0
roundness = 2.0

